<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE nta PUBLIC '-//Uppaal Team//DTD Flat System 1.1//EN' 'http://www.it.uu.se/research/group/darts/uppaal/flat-1_2.dtd'>
<nta>
	<declaration>// Place global declarations here.
chan a;</declaration>
	<template>
		<name x="5" y="5">T1</name>
		<declaration>// Place local declarations here.
clock x;</declaration>
		<location id="id0" x="-144" y="-93">
			<name x="-154" y="-127">L1</name>
		</location>
		<location id="id1" x="-68" y="-93" color="#ff0000">
			<name x="-78" y="-127">L2</name>
		</location>
		<location id="id2" x="-68" y="-93">
		</location>
		<init ref="id0"/>
		<transition color="#ff0000">
			<source ref="id1"/>
			<target ref="id0"/>
			<label kind="guard" x="-102" y="-59">x==1</label>
			<label kind="synchronisation" x="-144" y="-59">a?</label>
			<nail x="-110" y="-51"/>
		</transition>
		<transition>
			<source ref="id0"/>
			<target ref="id2"/>
			<label kind="assignment" x="-119" y="-153">x=0</label>
			<nail x="-110" y="-136"/>
		</transition>
	</template>
	<template>
		<name>T2</name>
		<declaration>clock y;</declaration>
		<location id="id3" x="0" y="0">
			<name x="-10" y="-34">L3</name>
			<label kind="invariant" x="-10" y="17">y &lt;= 1</label>
		</location>
		<init ref="id3"/>
	</template>
	<template>
		<name>T3</name>
		<location id="id4" x="0" y="0">
		</location>
		<init ref="id4"/>
	</template>
	<template>
		<name>T4</name>
		<location id="id5" x="0" y="0">
		</location>
		<init ref="id5"/>
	</template>
	<system>system T1, T2, T3, T4;
    </system>
	<queries>
		<query>
			<formula></formula>
			<comment></comment>
		</query>
	</queries>
</nta>
